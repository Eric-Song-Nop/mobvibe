{
	"name": "@mobvibe/cli",
	"version": "0.0.0-dev",
	"description": "CLI daemon for remote-claude (mobvibe) - connects ACP agents to the gateway",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Eric-Song-Nop/mobvibe.git",
		"directory": "apps/mobvibe-cli"
	},
	"keywords": [
		"acp",
		"agent-client-protocol",
		"mobvibe",
		"claude",
		"cli"
	],
	"engines": {
		"bun": ">=1.0.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"bin": {
		"mobvibe": "bin/mobvibe.mjs"
	},
	"files": [
		"dist",
		"bin"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"scripts": {
		"dev": "bun run build.ts",
		"build": "bun run build.ts",
		"build:bin": "bun run build-bin.ts",
		"start": "bun dist/index.js",
		"lint": "biome check --write .",
		"format": "biome format --write .",
		"test": "bun test",
		"test:run": "bun test",
		"prepublishOnly": "pnpm build"
	},
	"dependencies": {
		"@agentclientprotocol/sdk": "^0.12.0",
		"commander": "^13.1.0",
		"ignore": "^7.0.4",
		"open": "^10.1.0",
		"pino": "^9.6.0",
		"pino-pretty": "^13.1.1",
		"qrcode": "^1.5.4",
		"socket.io-client": "^4.8.1"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.11",
		"@mobvibe/shared": "workspace:*",
		"@types/bun": "latest",
		"@types/qrcode": "^1.5.5",
		"typescript": "~5.9.3"
	}
}
