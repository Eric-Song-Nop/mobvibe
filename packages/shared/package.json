{
	"name": "@mobvibe/shared",
	"version": "0.1.0",
	"description": "Shared types for remote-claude (mobvibe) distributed ACP system",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/user/remote-claude.git",
		"directory": "packages/shared"
	},
	"keywords": [
		"acp",
		"agent-client-protocol",
		"mobvibe"
	],
	"engines": {
		"node": ">=22"
	},
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsc -p tsconfig.json",
		"dev": "tsc -p tsconfig.json --watch",
		"lint": "biome check --write .",
		"lint:check": "biome check .",
		"format": "biome format --write .",
		"format:check": "biome format ."
	},
	"dependencies": {
		"@agentclientprotocol/sdk": "^0.14.1",
		"@noble/hashes": "^2.0.1",
		"tweetnacl": "^1.0.3",
		"zod": "^4.3.5"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.11",
		"@types/node": "^24.10.1",
		"typescript": "~5.9.3"
	}
}
