{
	"packageManager": "pnpm@10.27.0",
	"devDependencies": {
		"turbo": "^2.7.4"
	},
	"scripts": {
		"build": "turbo build",
		"dev": "NODE_ENV=development turbo dev",
		"format": "turbo format",
		"lint": "turbo lint",
		"test": "turbo test",
		"test:run": "turbo test:run",
		"tauri:dev": "pnpm --filter webui dev:tauri",
		"tauri:build": "pnpm --filter webui build:tauri",
		"android:init": "pnpm --filter webui android:init",
		"android:dev": "pnpm --filter webui android:dev",
		"ios:init": "pnpm --filter webui ios:init",
		"ios:dev": "pnpm --filter webui ios:dev"
	},
	"pnpm": {
		"overrides": {
			"@types/react": "~19.0.10"
		}
	}
}
